{
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["DOM", "ESNext"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "include": ["src"]
}
